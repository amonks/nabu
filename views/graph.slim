doctype html
meta charset="utf-8"
  css:
    html, body {
      margin: 0;
      padding: 0;
      top: 0;
      right: 0;
      left: 0;
      bottom: 0;
    }

    svg {
      vertical-align: middle;
      background: rgba(255,255,255, 0.2);
      box-shadow: inset 0 0 3px 0px #CECECE;
    }

    .axis path,
    .axis line {
      fill: none;
      stroke: #000;
      shape-rendering: crispEdges;
    }

    .line {
      fill: none;
      stroke-width: 2px;
      stroke: steelblue;
    }

  body
    script src="http://d3js.org/d3.v3.min.js"
    script src="/graph.js"
    javascript:
      var table = '#{{@table}}';
      var columns = #{{@columns}};
      var min = '#{{@min}}';
      var max = '#{{@max}}';
      var start = '#{{@start}}';
      var end = '#{{@end}}';
      graphIt(table, min, max, start, end);

